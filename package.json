{
  "name": "CMS Brave ag",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.17",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "ts-jest": "^29.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "debug": "ts-node-dev --respawn --transpile-only ./src/main/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:ci": "npm test -- --coverage",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
