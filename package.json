    "mysql2": "^2.3.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "sequelize": "^6.23.0",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
